FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform (0 0 0.5);

boundaryField
{
    leftBoundary
    {
        type            calculated;
        value           uniform (0 0 0.5);
    }
    rightBoundary
    {
        type            calculated;
        value           uniform (0 0 0.5);
    }
    emptyBoundary
    {
        type            empty;
    }
}


